<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html lang="ja-JP">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=<%=h charset() %>">
  <meta http-equiv="Content-Language" content="ja-JP">
  <link rel="stylesheet" type="text/css" href="<%=h css_url() %>">
  <title>Library Index</title>
</head>
<body>

<%
    headline_init
%>
<%= headline("Library Index") %>
<%
    headline_push
    lib = @entries.detect {|lib| lib.id == '_builtin' }
%>
<%= headline_noescape(library_link(lib.name, 'Builtin Library')) %>
<%= compile_rd(lib.synopsis_source) %>
<p>
classes/modules:
<%  lib.sorted_classes.each do |c| %>
<%=   class_link(c.name) %>
<%  end %>
</p>
<%
    @entries.each do |lib|
      next if lib.id == '_builtin'
%>
<%= headline_noescape(library_link(lib.name)) %>
<%= compile_rd(lib.synopsis_source) %>
<p>
classes/modules:
<%    lib.sorted_classes.each do |c| %>
<%=     class_link(c.name) %>
<%    end %>
</p>
<%
    end
    headline_pop
%>

</body>
</html>
