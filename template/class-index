</p>
<%
    headline_push
    @entries.each do |c|
%>
<%= headline_noescape("#{c.type} #{class_link(c.name)}") %>
<p>
<%
      c.sorted_entries {|m| m.id }.each do |m| %>
<%= method_link_short(m) %>
<%    end %>
</p>
<%
    end
    headline_pop
%>
</ul>

